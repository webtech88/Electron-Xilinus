{
  "name": "angular-electron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:web": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --bh . --op src/electron-hmr/dist --watch",
    "electron": "electron src/electron-hmr/main.dev.js",
    "electron:hmr": "cd src/electron-hmr && gulp serve",
    "build:prod": "ng build --base-href . && cp -r src/electron/* dist",
    "electron:prod": "electron dist",
    "release": "npm run release:mac",
    "release:win": "npm run build:prod && cd dist && electron-builder -c ../builder.json -w",
    "release:mac": "npm run build:prod && cd dist && electron-builder -c ../builder.json",
    "release:linux": "npm run build:prod && cd dist && electron-builder -c ../builder.json -l",
    "release:all": "npm run build:prod && cd dist && electron-builder -c ../builder.json -mwl",
    "clean": "rm -fr release/* dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.5",
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/forms": "^4.3.5",
    "@angular/http": "^4.3.5",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/router": "^4.3.5",
    "core-js": "^2.4.1",
    "d3": "^4.10.0",
    "electron": "^1.6.11",
    "lodash": "^4.17.4",
    "rxjs": "^5.4.3",
    "save": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.1",
    "@angular/compiler-cli": "^4.3.5",
    "@angular/language-service": "^4.3.5",
    "@types/d3": "^4.10.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.24",
    "@types/underscore": "^1.8.2",
    "codelyzer": "^3.1.2",
    "electron-builder": "^19.22.1",
    "electron-connect": "^0.6.2",
    "electron-log": "^2.2.7",
    "electron-updater": "^2.8.7",
    "gulp": "^3.9.1",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
